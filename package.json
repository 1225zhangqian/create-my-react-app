{
  "name": "create-qian-react-app",
  "version": "1.0.18",
  "description": "Create SFx React apps with no build configuration.",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "release": "standard-version",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s -r 0 && git add CHANGELOG.md ",
    "commit": "git-cz",
    "commitmsg": "validate-commit-msg"
  },
  "bin": {
    "create-qian-react-app": "./bin/createSfxApp.js"
  },
  "author": "Lucy",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "https://github.com/1225zhangqian/create-my-react-app",
    "directory": "create-my-react-app"
  },
  "dependencies": {
    "chalk": "4.0.0",
    "commander": "4.1.0",
    "conventional-changelog": "^3.1.21",
    "conventional-changelog-cli": "^2.0.34",
    "envinfo": "7.5.1",
    "hyperquest": "2.1.3",
    "inquirer": "7.1.0",
    "replace": "^1.2.0",
    "tar-pack": "3.4.1",
    "tmp": "0.2.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^9.0.1",
    "@commitlint/config-angular": "^9.0.1",
    "commitizen": "^4.1.2",
    "cz-conventional-changelog": "^3.2.0",
    "husky": "^4.2.5",
    "standard-version": "^8.0.0",
    "validate-commit-msg": "^2.14.0"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
